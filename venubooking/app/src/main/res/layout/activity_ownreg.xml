<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="panduchinnu.venubooking.ownreg"
    android:background="@drawable/backgroundg">


    <TextView
        android:id="@+id/ot1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="33dp"
        android:text="Name:"
        android:textStyle="bold"
        android:textSize="18dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/ot2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:text="Password:"
        android:textStyle="bold"
        android:textSize="18dp"
        android:layout_below="@+id/ot1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/ot4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email Id:"
        android:textStyle="bold"
        android:textSize="18dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/ot5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:text="Mobile No:"
        android:layout_below="@+id/ot4"
        android:layout_alignLeft="@+id/ot4"
        android:layout_alignStart="@+id/ot4"
        android:textStyle="bold"
        android:textSize="18dp"/>

    <TextView
        android:id="@+id/ot6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address:"
        android:textStyle="bold"
        android:textSize="18dp"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/editText12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/ot7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Aadhar No:"
        android:layout_below="@+id/ot6"
        android:layout_alignLeft="@+id/ot6"
        android:layout_alignStart="@+id/ot6"
        android:textStyle="bold"
        android:textSize="18dp"/>

    <EditText
        android:id="@+id/edit9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textColor="#093453"
        android:inputType="text"
        android:layout_alignBaseline="@+id/ot1"
        android:layout_alignBottom="@+id/ot1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textColor="#093453"
        android:inputType="textPassword"
        android:layout_alignBaseline="@+id/ot2"
        android:layout_alignBottom="@+id/ot2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textColor="#093453"
        android:inputType="textEmailAddress"
        android:hint="This will be ur username"
        android:layout_alignBaseline="@+id/ot4"
        android:layout_alignBottom="@+id/ot4"
        android:layout_alignLeft="@+id/editText12"
        android:layout_alignStart="@+id/editText12" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textColor="#093453"
        android:inputType="phone"
        android:layout_alignBaseline="@+id/ot5"
        android:layout_alignBottom="@+id/ot5"
        android:layout_alignLeft="@+id/editText10"
        android:layout_alignStart="@+id/editText10" />

    <EditText
        android:id="@+id/editText13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textColor="#093453"
        android:inputType="textPersonName"
        android:layout_alignBaseline="@+id/ot6"
        android:layout_alignBottom="@+id/ot6"
        android:layout_alignLeft="@+id/editText12"
        android:layout_alignStart="@+id/editText12" />

    <EditText
        android:id="@+id/editText14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textColor="#093453"
        android:inputType="number"
        android:layout_alignBaseline="@+id/ot7"
        android:layout_alignBottom="@+id/ot7"
        android:layout_alignLeft="@+id/editText13"
        android:layout_alignStart="@+id/editText13" />

    <TextView
        android:id="@+id/ot3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Confirm password:"
        android:textStyle="bold"
        android:textSize="18dp"
        android:layout_above="@+id/editText11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="36dp" />

    <EditText
        android:id="@+id/editText15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textColor="#093453"
        android:inputType="textPassword"
        android:layout_alignTop="@+id/ot3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="35dp"
        android:layout_marginStart="35dp"
        android:layout_toEndOf="@+id/ot7"
        android:layout_toRightOf="@+id/ot7"
        android:text="Submit"
        android:textStyle="bold"
        android:textSize="18dp"/>
</RelativeLayout>
